#!/usr/bin/env ruby

require 'fileutils'
$LOAD_PATH.unshift File.expand_path("../app", __dir__)
require "vr"

FileUtils.mkdir_p "cache/fetcher"
FileUtils.mkdir_p "cache/dataset"
ds = VR::Dataset.new("datasets.json")

print "Starting cache save\n"
ds.each do |entry|
  print "Saved #{entry[:name]}\n"
end
